# Axie Studio Environment Configuration

# Backend Configuration
LANGFLOW_BACKEND_URL=http://localhost:7860
BACKEND_URL=http://localhost:7860

# Authentication Configuration
AXIE_STUDIO_AUTO_LOGIN=false

# User Management Configuration
# Superuser credentials (default admin user)
AXIE_STUDIO_SUPERUSER=stefan@axiestudio.se
AXIE_STUDIO_SUPERUSER_PASSWORD=STEfanjohn!12

# Additional users (format: username:password:role,username:password:role)
# Roles: 'user' or 'superuser'
AXIE_STUDIO_USERS=user1:password1:user,user2:password2:user,user3:password3:superuser

# Token Configuration
ACCESS_TOKEN_EXPIRE_SECONDS=3600

# Development Configuration
CI=false

# Deployment Configuration (for Digital Ocean)
NODE_ENV=production
PORT=3000

# Optional: Custom branding
AXIE_STUDIO_TITLE="Axie Studio"
AXIE_STUDIO_DESCRIPTION="AI Flow Builder"

# Optional: Feature flags
ENABLE_FILE_MANAGEMENT=true
ENABLE_CUSTOM_PARAM=false
